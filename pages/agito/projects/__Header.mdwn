#{{$title}}
{{$description}}

#Tasks

[[!sqlreport
database=tasks
table=tasks
where="""
full_project = '{{$name}}'
and status != 'deleted'
"""
headers="{$status}"
header_start=2
sort_by="status -urgency -priorityval due wait id"
table_class=data
table_header=" "
row_template="""
<tr {?has_depends class="depends"}>
<td><strong>({$id})</strong></td>
<td>
{?priority [$priority]}
{?urgency ([$urgency])}
</td>
<td>
{?wait Wait:[$waitdate]}
{?due Due:[$duedate]}
{?start Start:[$startdate]}
{?end End:[$enddate]}
</td>
<td>
{?depends Depends:[$depends:pipetocomma]}
{?tags ([$tags:items_wlink_tag])}
</td></tr>
<tr {?has_depends class="depends"}>
<td colspan=4><em>{$description} {?annotations :: [$annotations]}</em>
</td>
</tr>
"""]]

