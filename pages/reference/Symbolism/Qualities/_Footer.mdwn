[[!sqlreport
database=muster
table=flatfields
where="""
parent_page = 'reference/Symbolism/Qualities'
AND (SeeAlso = '{{$name}}'
OR SeeAlso LIKE '%|{{$name}}'
OR SeeAlso LIKE '%|{{$name}}|%'
OR SeeAlso LIKE '{{$name}}|%')
"""
sort_by="page"
layout=list report_style=compact
row_template="""
<li><b>[[{$page}]]</b>
<small>{$description}</small></li>
"""]]
