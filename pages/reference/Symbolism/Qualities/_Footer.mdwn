[[!sqlreport
database=muster
table=flatfields
where="""
parent_page = 'reference/Symbolism/Symbols'
AND (Qualities = '{{$name}}'
OR Qualities LIKE '%|{{$name}}'
OR Qualities LIKE '%|{{$name}}|%'
OR Qualities LIKE '{{$name}}|%')
"""
sort_by="page"
layout=list
report_style=bare
ltemplate="""
<b>Symbols:</b>

CONTENTS
"""
row_template="""
<li><b>[[{$page}]]</b>
{?Qualities [$Qualities:items_wlink_qualities]}
{?HasQualitiesOf ([$HasQualitiesOf:items_wlink_symbols])}
<br/><small>{$description}</small>
</li>
"""]]

