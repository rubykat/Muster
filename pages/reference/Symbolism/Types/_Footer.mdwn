[[!sqlreport
database=muster
table=flatfields
where="""
parent_page = 'reference/Symbolism/Symbols'
AND (SymbolType = '{{$name}}'
OR SymbolType LIKE '%|{{$name}}'
OR SymbolType LIKE '%|{{$name}}|%'
OR SymbolType LIKE '{{$name}}|%')
"""
sort_by="page"
layout=list
row_template="""
<li><b>[[{$page}]]</b>
{?Qualities [$Qualities:items_wlink_qualities]}
{?HasQualitiesOf ([$HasQualitiesOf:items_wlink_symbols])}
<em>{$SymbolType:items_wlink_types}</em>
<br/><small>{$description}</small>
</li>
"""]]
